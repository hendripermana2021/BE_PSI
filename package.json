{
  "name": "be_psi",
  "version": "1.0.0",
  "description": "for Skripsi",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:create": "npx sequelize db:create",
    "dev": "nodemon ./index.js",
    "start": "node ./index.js",
    "tbl_user": "npx sequelize-cli model:generate --name tbl_user --attributes name:string,sex:string,email:string,province_id:integer,region_id:integer,password:string,real_password:string,role_id:integer,accesstoken:text,refreshtoken:text",
    "tbl_program": "npx sequelize-cli model:generate --name tbl_program --attributes name_program:string,total_dana_alokasi:float",
    "tbl_req_ajuan": "npx sequelize-cli model:generate --name tbl_req_ajuan --attributes id_users:integer,id_province:integer,id_region:integer,id_program:integer,jlh_dana:float,psi_result:float,commented:string,req_status:boolean,rank:integer,id_calculated:integer",
    "tbl_kriteria": "npx sequelize-cli model:generate --name tbl_kriteria --attributes name_kriteria:string,weight_score:float,type:boolean",
    "tbl_subkriteria": "npx sequelize-cli model:generate --name tbl_subkriteria --attributes name_sub:string,id_kriteria:integer,description:text,value:float",
    "tbl_psi": "npx sequelize-cli model:generate --name tbl_psi --attributes id_kriteria:integer,id_subkriteria:integer,comment:text,id_order:integer",
    "tbl_notification": "npx sequelize-cli model:generate --name tbl_notification --attributes user_id:integer,message:string,isRead:boolean",
    "tbl_role": "npx sequelize-cli model:generate --name tbl_role --attributes role_name:string",
    "tbl_calculated": "npx sequelize-cli model:generate --name tbl_calculated --attributes created_by:integer",
    "tbl_province": "npx sequelize-cli model:generate --name tbl_province --attributes name_province:string",
    "tbl_region": "npx sequelize-cli model:generate --name tbl_region --attributes id_province:integer,name_region:string",
    "tbl_program_kriteria": "npx sequelize-cli model:generate --name tbl_program_kriteria --attributes id_program:integer,id_kriteria:integer",
    "seed-user": "npx sequelize-cli seed:generate --name demo-tbl_user",
    "seed-program": "npx sequelize-cli seed:generate --name demo-tbl_program",
    "seed-req_ajuan": "npx sequelize-cli seed:generate --name demo-tbl_req_ajuan",
    "seed-kriteria": "npx sequelize-cli seed:generate --name demo-tbl_kriteria",
    "seed-sub_kriteria": "npx sequelize-cli seed:generate --name demo-tbl_sub_kriteria",
    "seed-psi": "npx sequelize-cli seed:generate --name demo-tbl_psi",
    "seed-notification": "npx sequelize-cli seed:generate --name demo-tbl_notification",
    "seed-role": "npx sequelize-cli seed:generate --name demo-tbl_role",
    "seed-calculated": "npx sequelize-cli seed:generate --name demo-tbl_calculated",
    "seed-province": "npx sequelize-cli seed:generate --name demo-tbl_province",
    "seed-region": "npx sequelize-cli seed:generate --name demo-tbl_region",
    "seed-program-kriteria": "npx sequelize-cli seed:generate --name demo-tbl_program_kriteria",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:undo": "npx sequelize-cli db:migrate:undo:all",
    "db:drop": "npx sequelize-cli db:drop",
    "db:restart": "pnpm db:drop && pnpm db:create && pnpm db:migrate && pnpm db:seed && pnpm dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-loader": "^9.1.3",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "fs": "^0.0.1-security",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.1",
    "latest": "^0.2.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.6.0",
    "nodemon": "^3.1.7",
    "part": "^0.1.1",
    "path": "^0.12.7",
    "pg": "^8.13.0",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "socket.io": "^4.7.2",
    "swagger": "^0.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  }
}
